{
  "name": "maps-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "backend": "json-server --watch data/db.json",
    "envs": "node ./scripts/set-envs.js",
    "start": "ng serve",
    "build": "ng build",
    "build:href": "ng build --base-href ./",
    "build:github": "npm run build:href && npm run reorder:dist && npm run delete:docs && npm run copy:dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "reorder:dist": "npm run copy:browsercontent && npm run delete:browser",
    "copy:browsercontent": "copyfiles dist/maps-app/browser/* dist/maps-app -f",
    "delete:browser": "del-cli dist/maps-app/browser",
    "copy:dist": "copyfiles dist/maps-app/* ./docs -f",
    "delete:docs": "del-cli docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "dotenv": "^16.4.5",
    "mapbox-gl": "^3.7.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/mapbox-gl": "^3.4.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}
